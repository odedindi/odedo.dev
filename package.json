		"@types/nprogress": "^0.2.0",
		"@types/react": "^17.0.39",
		"@types/styled-components": "^5.1.24",
		"apollo": "^2.33.6",
		"babel-plugin-styled-components": "^1.13.2",
		"eslint": "^8.10.0",
		"eslint-config-next": "^12.1.0",
		"ts-node-dev": "^1.1.8",
		"typescript": "4.4.2"
	}
