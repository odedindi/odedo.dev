{
	"name": "starter",
	"version": "0.2.1",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write .",
		"check:types": "tsc --pretty --noEmit",
		"check:lint": "next lint",
		"check:format": "prettier --check .",
		"check:all": "yarn check:types && yarn check:lint && yarn check:format",
		"check:build": "yarn check:all && yarn build",
		"generate:types": "npx apollo codegen:generate --localSchemaFile=schema.graphql --target=typescript --tagName=gql"
	},
	"dependencies": {
		"@apollo/client": "^3.8.8",
		"@mantine/core": "^3.6.14",
		"@mantine/dates": "^3.6.14",
		"@mantine/hooks": "^3.6.14",
		"@mantine/next": "^3.6.14",
		"@mantine/notifications": "^3.6.14",
		"@mantine/prism": "^3.6.14",
		"@modulz/radix-icons": "^4.0.0",
		"apollo-server-micro": "^3.13.0",
		"dayjs": "^1.11.10",
		"graphql": "^16.8.1",
		"graphql-scalars": "^1.22.4",
		"gsap": "^3.12.4",
		"i18next": "^23.10.0",
		"lodash": "^4.17.21",
		"micro": "^10.0.1",
		"next": "^14.0.4",
		"next-i18next": "^15.2.0",
		"next-seo": "^6.4.0",
		"nexus": "^1.3.0",
		"nprogress": "^0.2.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-i18next": "^14.0.5",
		"react-query": "^3.39.3",
		"react-social-icons": "^6.10.0",
		"styled-components": "^6.1.1"
	},
	"devDependencies": {
		"@types/graphql": "^14.5.0",
		"@types/lodash": "^4.14.202",
		"@types/nprogress": "^0.2.3",
		"@types/react": "^18.2.45",
		"@types/styled-components": "^5.1.34",
		"apollo": "^2.34.0",
		"babel-plugin-styled-components": "^2.1.4",
		"eslint": "^8.56.0",
		"eslint-config-next": "^14.0.4",
		"prettier": "^3.1.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "5.3.3"
	},
	"packageManager": "yarn@4.0.2"
