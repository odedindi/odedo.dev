{
	"name": "starter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write .",
		"check:types": "tsc --pretty --noEmit",
		"check:lint": "next lint",
		"check:format": "prettier --check .",
		"check:all": "yarn check:types && yarn check:lint && yarn check:format",
		"check:build": "yarn check:all && yarn build",
		"generate:types": "npx apollo codegen:generate --localSchemaFile=schema.graphql --target=typescript --tagName=gql"
	},
	"dependencies": {
		"@apollo/client": "^3.7.2",
		"@mantine/core": "^3.6.14",
		"@mantine/dates": "^3.6.14",
