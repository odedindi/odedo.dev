		"@mantine/next": "^3.6.14",
		"@mantine/notifications": "^3.6.14",
		"@mantine/prism": "^3.6.14",
		"@modulz/radix-icons": "^4.0.0",
		"apollo-server-micro": "^3.3.0",
		"dayjs": "^1.10.8",
		"graphql": "^15.5.3",
		"graphql-scalars": "^1.15.0",
		"gsap": "^3.9.1",
		"lodash": "^4.17.21",
		"micro": "^9.3.4",
		"next": "^12.1.0",
		"next-seo": "^5.1.0",
		"nexus": "^1.1.0",
		"nprogress": "^0.2.0",
		"prettier": "^2.5.1",
