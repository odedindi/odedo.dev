{
	"presets": ["next/babel"],
	"plugins": [
		[
			"babel-plugin-styled-components",
			{
				"ssr": true,
				"minify": true,
				"transpileTemplateLiterals": true,
				"pure": true,
				"displayName": true,
				"preprocess": false
			}
		],
		"babel-plugin-polished",
		[
			"module-resolver",
			{
				"alias": {
					"components": "./components"
				}
			}
		]
	]
}
